PRATICANDO FUNCTION 11/10/24 https://codepen.io/feef-the-styleful/pen/QWeNJKQ


//praticando function, essa function vai somar numeros ou subtrair, dependendo dos numeros, parseint vão transformar numeros de string para number, e parse float transforma números decimais.

function soma(num1, num2) {
  return num1 + num2;
}
function menos(num1, num2) {
  return num1 - num2;
}

const compra1 = prompt("qual o valor da primeira compra?");
const compra2 = prompt("qual o valor da segunda compra?");

const compra1formatada = parseFloat(compra1);
const compra2formatada = parseFloat(compra2);

if (compra1formatada > compra2formatada) {
  console.log(soma(compra1formatada, compra2formatada));
} else if (compra1formatada < compra2formatada) {
  console.log(menos(compra1formatada, compra2formatada));
} else {
  console.log("erro po");
}



FUNCTION 07/10/24 https://codepen.io/feef-the-styleful/pen/GRbeKjj


//essa função replica coisas(toda vez que chamar a função e colocar qualquer informação dentro, ela vai responder com "nao".)

function feef() {
  return "vamos ver suas notas";
}

console.log(feef("quero somar minhas notas"));

//essa função muntiliplica qualquer numero colocado em 2

function dobro(num) {
  return num * 2;
}
console.log(dobro(3));

//essa função muntiliplica qualquer numero colocado em 3

function triplo(num) {
  return num * 3;
}
console.log(triplo(3));

//essa função consegue dividir ou multiplicar notas ou numbers diferentes.

function calcularMedia(nota1, nota2, nota3) {
  const soma = nota1 + nota2 + nota3;
  const media = soma / 3;
  return media;
}

console.log(calcularMedia(1, 23, 80));

//função arrow

const falar = () => "oi tudo bom";

const quadruplo = (num) => num * 4;

console.log(falar("ola"));
console.log(quadruplo(2));




IF ELSE ELSE IF 07/10/24 https://codepen.io/feef-the-styleful/pen/ZEgzZxP


// se if for falso, o console irá executar o else.
const morte = "nao";
const resposta = "aaaa";
const pergunta = prompt("aaaa?");
if ("aaaa") {
  console.log(resposta);
} else {
  console.log(morte);
}


PRATICANDO FUNCTION 07/10/24 https://codepen.io/feef-the-styleful/pen/QWeNJKQ

//praticando function, essa function vai somar numeros ou subtrair, dependendo dos numeros, parseint vão transformar numeros de string para number, e parse float transforma números decimais.

function soma(num1, num2) {
  return num1 + num2;
}
function menos(num1, num2) {
  return num1 - num2;
}

const compra1 = prompt("qual o valor da primeira compra?");
const compra2 = prompt("qual o valor da segunda compra?");

const compra1formatada = parseFloat(compra1);
const compra2formatada = parseFloat(compra2);

if (compra1formatada > compra2formatada) {
  console.log(soma(compra1formatada, compra2formatada));
} else if (compra1formatada < compra2formatada) {
  console.log(menos(compra1formatada, compra2formatada));
} else {
  console.log("erro po");
}






