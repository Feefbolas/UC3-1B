FUNCTION + SWITCH CASE https://codepen.io/feef-the-styleful/pen/QWeNJKQ


function feriado(feriados) {
  switch (feriados) {
    case "janeiro":
      alert("Dia da Confraternização Universal");
      break;
    case "fevereiro":
      alert("carnaval, quarta feira de cinzas");
      break;
    case "março":
      alert("sexta feira santa, dia da mulher");
      break;
    case "abril":
      alert("dia do tiradentes");
      break;
    case "maio":
      alert("Corpus Christ");
      break;
    case "junho":
      alert("São João");
      break;
    case "julho":
      alert("não tem :_:");
      break;
    case "agosto":
      alert("não tem :_:");
      break;
    case "setembro":
      alert("Dia de Nossa Senhora das Dores");
      break;
    case "outubro":
      alert("Dia de Nossa Senhora de Aparecida");
      break;
    case "novembro":
      alert("Dia do finados, Proclamação da República");
      break;
    case "dezembro":
      alert("natal");
      break;
  }




ARRAY + FUNCTION https://codepen.io/feef-the-styleful/pen/ZEgpXjK

//array

function entrarJogo(listaDeJogos) {
  return listaDeJogos.join(" metal gear ");
}

let listaDeJogos = ["god of war", "shadow of the colossus", "far cry 3"];

console.log(entrarJogo(listaDeJogos));



SITUAÇÃO PROBLEMA https://codepen.io/feef-the-styleful/pen/VwJqWzB
//situação problema: quero saber quem e quantos são os integrantes homens do meu grupo , e quem são os integrantes mais altos.

var crepusculo = ["fernando", "gustabo", "mamao", "BENTO", "MBAPPE", "heitor"];

var altos = crepusculo.slice(1, 4);

console.log(altos);

console.log(crepusculo[0]);
console.log(crepusculo[1]);
console.log(crepusculo[2]);
console.log(crepusculo[3]);
console.log(crepusculo[4]);
console.log(crepusculo[5]);

var tamanho = crepusculo.length;
console.log(tamanho);



ATIVIDADE ANIME/JOGO/BRINCADEIRA https://codepen.io/feef-the-styleful/pen/RwXbbry


<html>

