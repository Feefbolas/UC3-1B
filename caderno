const escolha = prompt("qual fruta você deseja?");
var listadefrutas = ["maçã", "banana", "jabuticaba"];
var pos = listadefrutas.indexOf(escolha.toLowerCase());

console.log(`suas ${listadefrutas[pos]} estão prontas para entrega`);

