function ataque() {
  return "voce bate nele, ele não sente nada e te mata";
}

var ação = prompt(
  "você é um homem loiro chamado hakari, você decidiu andar por ai procurando gente para espancar, um homem extremamente palido e musculoso te chamou atenção, o que voce deseja fazer?"
);
if (ação == "bater nele") {
  console.log(ataque(ataque));
} else if (ação == "não mexer com ele") {
  console.log("você ignora e vai embora");
} else {
  console.log("ele te ve olhando pra ele, e te mata");
}


